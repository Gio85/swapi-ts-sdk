{
  "name": "swapi-ts-sdk",
  "version": "1.0.3",
  "description": "SDK to fetch SWAPI in typescript",
  "license": "MIT",
  "private": false,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "start:dev": "ts-node-dev --respawn --transpile-only local.ts",
    "test": "jest --config ./jest_unit.json",
    "integration:dev": "jest --config ./jest_integration.json --watch",
    "integration": "jest --config ./jest_integration.json"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "dotenv": "^8.2.0",
    "qs": "^6.9.6"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^5.0.1",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.31",
    "@types/qs": "^6.9.5",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "eslint": "^7.20.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.2",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.2.2"
  }
}
